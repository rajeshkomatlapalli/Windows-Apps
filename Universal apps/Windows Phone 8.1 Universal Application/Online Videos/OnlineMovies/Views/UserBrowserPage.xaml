<Page
    x:Class="OnlineMovies.Views.UserBrowserPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:OnlineMovies.Views"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">

    <Grid x:Name="LayoutRoot" Background="{StaticResource bipivot}">
        <Button x:Name="back" Content="Back" Click="back_Click" VerticalAlignment="Top" Margin="0,-10,0,0"/>
        <WebView x:Name="webBrowser1" Height="630" VerticalAlignment="Bottom" Width="400" LoadCompleted="webBrowser1_LoadCompleted" NavigationStarting="webBrowser1_NavigationStarting" ScriptNotify="webBrowser1_ScriptNotify"></WebView>
    </Grid>
</Page>
