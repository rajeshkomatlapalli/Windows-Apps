            <!--<StackPanel x:Name="playerpanel" Orientation="Horizontal">-->
            <player:MediaPlayer x:ConnectionId='1' x:Name="mediaplayer"                                                                                                                ></player:MediaPlayer>
                <!--<MediaElement x:Name="mediaplayer1" MediaEnded="mediaplayer_MediaEnded" MediaFailed="mediaplayer_MediaFailed" MediaOpened="mediaplayer_MediaOpened"></MediaElement>-->
            <!--</StackPanel>-->
        </Grid>
    </Grid>
</Page>

